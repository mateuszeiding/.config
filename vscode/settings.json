{
  "workbench.iconTheme": "vscode-icons",
  "editor.formatOnSave": true,
  "vscode-pets.theme": "beach",
  "vscode-pets.petColor": "lightbrown",
  "vscode-pets.petSize": "medium",
  "vscode-pets.throwBallWithMouse": true,
  "workbench.colorTheme": "Ros√© Pine Moon",
  "window.zoomLevel": 1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "accessibility.signals.diffLineModified": {
    "sound": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off"
  },
  "accessibility.signals.chatRequestSent": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.progress": {
    "sound": "off",
    "announcement": "off"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "git.replaceTagsWhenPull": true,
  "editor.lineNumbers": "relative",
  "diffEditor.ignoreTrimWhitespace": false,
  "git.confirmSync": false,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.inlayHints.enabled": "on",
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.inlayHints.optionsWrapper": true,
  "vue.autoInsert.dotValue": true,
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.missingProps": true,
  "vue.inlayHints.vBindShorthand": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "workbench.statusBar.visible": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "vim.leader": " ",
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-c>"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<C-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["m", "z", "J", "`", "z"]
      // Join line below to current, but keep cursor in place
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
      // Scroll down and center cursor
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
      // Scroll up and center cursor
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z", "z", "v"]
      // Next search result and center it
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z", "z", "v"]
      // Previous search result and center it
    },
    {
      "before": ["<leader>", "P"],
      "after": ["\"", "+", "P"]
      // Paste from system clipboard before cursor
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "+", "p"]
      // Paste from system clipboard after cursor
    },
    {
      "before": ["Q"],
      "after": []
      // Disable Q (Ex mode)
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "_", "d", "P"]
      // Paste over selection without overwriting yank buffer
    },
    {
      "before": ["<leader>", "y"],
      "after": ["\"", "+", "y"]
      // Yank selection to system clipboard
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
      // Delete selection without yanking
    }
  ],
  "eslint.format.enable": true,
}
